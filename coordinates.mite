struct Coordinate {
    x: f32,
    y: f32
}

export fn makeCoordinate(x: f32, y: f32): Coordinate {
    return js Coordinate { x, y };
}

export fn addCoordinates(a: Coordinate, b: Coordinate): Coordinate {
    return js Coordinate { x: a.x + b.x, y: a.y + b.y };
}

export fn distance(a: Coordinate, b: Coordinate): f32 {
    let dx = a.x - b.x;
    let dy = a.y - b.y;
    return sqrt(dx * dx + dy * dy);
}
